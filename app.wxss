image{
    display: block;
}
.container {
  width:100%;
  height: 100%;
  padding: 0;
  margin: 0;
}

.fc-40 {
    color: #404040;
}
.fc-f60 {
    color: #f60;
}

.addLine {
  position: relative;
  margin-left: 13px;
}

.com-margin-l {
    margin-left: 10px;
}

.com-border-right:after {
    right: 0;
    top: 0;
    height: 100%;
    width: 1px;
    background-color: #e6e6e6;
    -webkit-transform-origin: 100% 0;
    -ms-transform-origin: 100% 0;
    transform-origin: 100% 0;
    -webkit-transform: scale3d(0.5, 1, 1);
    -ms-transform: scale3d(0.5, 1, 1);
    transform: scale3d(0.5, 1, 1);
}
text {
    font-size: 1em;
    font-style: normal;
    font-weight: 500;
}

.com-flex-item,
.com-flex-list li {
    -webkit-box-flex: 1;
    -ms-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
    display: block;
    display: table-cell\9;
    overflow: hidden;
}
.com-flex,
.com-flex-list {
    display: -webkit-box;
    display: -ms-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}